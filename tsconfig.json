{
	"compilerOptions": {
		"target": "ES2020",
		"module": "commonjs",
		"moduleResolution": "node",
		"jsx": "react-jsx",
		"strict": true,
		"allowJs": false,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"noEmit": true,
		"isolatedModules": true,
		"types": ["node", "electron"]
	},
	"include": ["src"]
}